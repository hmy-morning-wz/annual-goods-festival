.am-modal-show {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1101;
}
.am-modal-hide {
  display: none;
}
.am-modal-mask,
.am-modal-document {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.am-modal-mask {
  background-color: rgba(0, 0, 0, 0.75);
}
.am-modal-document {
  display: flex;
  align-items: center;
  justify-content: center;
}
.am-modal {
 /* background-color: #fff;*/
  border-radius: 20rpx;
  width:640rpx;
  background: white;
  position: relative;
}
.am-modal-image {
  display: flex;
  justify-content: center;
}
.am-modal-image-md,
.am-modal-image-sm {
  margin-top: 24px;
}
 
.am-modal-image-lg image {
  width: 270px;
  height: 156px;
}
.am-modal-image-md image {
  width: 134px;
  height: 134px;
}
.am-modal-image-sm image {
  width: 65px;
  height: 65px;
}
.am-modal-header {
  font-size: 18px;
  line-height: 21px;
  text-align: center;
  color: #333;
  font-weight: 600;
  padding-top: 22px;
}
.am-modal-body {
  margin-top: 0px;
  overflow: hidden;
  max-width:640rpx;
  padding: 0 0px;
  line-height: 20px;
  text-align: center;
  color: #333;
  font-size: 14px;
}
.am-modal-kf {
position: absolute;
  left: 50%;
  width: 100%;
  transform : translate(-50%,0);
text-align: center;
top: 478rpx;
height:48rpx;
font-size:20rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(151,160,175,1);
line-height:48rpx;
}
.am-modal-footer {
  position: absolute;
  margin-top: 0px;
  left: 50%;
  transform : translate(-50%,0);
  width: 480rpx;
  top: 546rpx;
  height: 80rpx;
  text-align: center;
  background:linear-gradient(180deg,#FFC000 0%,#EA9B00 100%);
  box-shadow:0px 8rpx 16rpx 0px #FFC000;
  border-radius:40rpx;  
  font-size:32rpx;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:80rpx;
}
.am-modal-more {
  position: relative;
  left: 50%;
  width: 100%;
  transform : translate(-50%,0);
  height: 200rpx;
}


.banner {
  width: 616rpx;
  height: 128rpx;
}
.am-modal-more .msg{
height:48rpx;
font-size:20rpx;
font-family:PingFangSC-Regular,PingFang SC;
font-weight:400;
color:rgba(151,160,175,1);
line-height:48rpx;
}

.am-modal-close {
  position: absolute;
  display: block;
  left: 50%;
  transform : translate(-50%,0);
  bottom: -50px;
  height: 48px;
  width: 48px;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 16px 16px;
}

.am-close-white {
  background: url("https://gw.alipayobjects.com/zos/rmsportal/fdmdsySxNBrpcVBVGEUM.png") center no-repeat;;
  background-size: 16px 16px;
}

.am-modal-close-active,
.am-modal-footer-active {
  background-color: rgba(37,39,40,.05);
}

.am-modal-document-advice .am-modal {
  padding-top: 0;
  width: auto;
}
.am-modal-document-advice .am-modal-body {
  padding: 0;
  max-width:640rpx;
}

.am-modal-document-advice .am-modal-close {
  /*top: -50px;*/
  height: 27px;
  width: 27px;
  background-size: 27px 27px;
  background-image: url("data:image/png;base64,");
}


